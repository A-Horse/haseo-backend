{
  "name": "haseo-cli",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prepublish": "npm run compile",
    "tocompile": "node_modules/.bin/babel -d ./dist",
    "compile": "node_modules/.bin/babel -d ./dist ./src",
    "watch-compile": "watchman-make -p 'src/**/*.js' --make='npm run tocompile' -s 2 -t='./src'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "pm2 start main.js --interpreter ./node_modules/.bin/babel-node --name=\"haseo-server\"",
    "stop": "pm2 stop haseo-server",
    "restart": "pm2 restart haseo-server"
  },
  "bin": {
    "haseo": "bin/haseo"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/REPOS/"
    ]
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-types": "^6.21.0"
  },
  "dependencies": {
    "babel-preset-stage-1": "^6.24.1",
    "bluebird": "^3.5.0",
    "colors": "^1.1.2",
    "es6-promise": "^4.1.0",
    "express": "^4.15.3",
    "jest": "^21.0.2",
    "js-yaml": "^3.10.0",
    "knex": "^0.13.0",
    "moment": "^2.18.1",
    "optimist": "^0.6.1",
    "ramda": "^0.24.1",
    "sqlite3": "^3.1.13",
    "winston": "^3.0.0-rc1",
    "ws": "^3.0.0",
    "yamljs": "^0.3.0"
  }
}
